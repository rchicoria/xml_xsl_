<script type="text/javascript">

    var selectedMenu = infoMenu.getMenuIdByUrl(window.location);
    if(selectedMenu != ""){
        jQuery('#'+selectedMenu).attr('href','#');
        jQuery('#'+selectedMenu).removeClass()
        jQuery('#'+selectedMenu).addClass('linkElementSelected')
    }
