<script type="text/javascript">

/**
 * Validar o endereço de email correto
 * @param  - String
 * @return boolean
 */
function validateEmailAddress(mail){
    var patternEmail = /^(\w|\.|-)+@(\w|\.|-)+(\.[a-zA-Z]{2,4})$/;
    
    if(mail == null || mail == '' || !patternEmail.test(mail)){
        return false;
    }

    return true;
}


    /**
     * Chamada no poplogin para recuperação de password.
     * @return
     */
    function sendRecoveryEmail() {
        jQuery('#msgNotifica').hide();
        jQuery('#msgNotifica').html('');
        jQuery('#msgLoading').show();

        var msgErroGeral = 'O endereço digitado não se encontra nos nossos registos.';
        var msgSuccess = 'A sua password foi enviada para o mail ';

        // Verifica o email submetido pelo utilizador
        var email = trim(jQuery('#checkout_user_input').val());
    
        if (!validateEmailAddress(email)){
           jQuery('#msgNotifica').html('Verifique o e-mail introduzido.');
           jQuery('#checkout_user_input').focus();
           jQuery('#msgNotifica').show();
           jQuery('#msgLoading').hide();
           return false;
        }
       

        var postURL = '/home/doRecoveryPassword';

        jQuery.ajax({
            cache:false,
            async:false,
            type: "POST",
            data: {email: email},
            url: postURL,
            success:    function(msg)
                        {
                           

                            if(msg == 1)
                            {
                                jQuery('#msgNotifica').html(msgSuccess + email);
                            }
                            else
                            {
                                jQuery('#msgNotifica').html(msgErroGeral);
                            }

                            jQuery('#msgLoading').hide();
                            jQuery('#msgNotifica').show();

                        },
            error:      function()
                        {
                           
                            jQuery('#msgNotifica').html(msgErroGeral);
                            jQuery('#msgLoading').hide();
                            jQuery('#msgNotifica').show();
                        }

        });
    }


    /*********************************************************************
     * Posiciona o login e atribui a redirecao pretendida
     */
    function openLoginPop(areaDestino,jsInfo){
        jQuery('#dialogCheckout').dialog('open');
        if(areaDestino != null){
            jQuery('#checkout_redirectUrl').val(areaDestino);
        }
        
        if(jsInfo != null){
            jQuery('#checkout_hiddenID').val(jsInfo);
        }

        jQuery("#dialogCheckout").dialog( "option", "position", [tempX,tempY]);
        return false;
    }

    jQuery("#dialogCheckout").dialog({
        bgiframe: true,
        autoOpen: false,
        width: 242,
        closeText: "FECHAR",
        close: function(ev, ui) {
            jQuery("#checkout_user_input").val('');
            jQuery("#checkout_password").val('');
            jQuery("#checkout_banner").html('');

            // Recuperar password
            jQuery('#msgNotifica').html('');            
            jQuery('#msgNotifica').hide();
            jQuery('#msgLoading').hide();
        }
    });

    jQuery(function(){
        jQuery("#CheckoutLoginForm").submit(function(){

            jQuery('#msgNotifica').hide();
            jQuery('#msgNotifica').html('');
            jQuery('#msgLoading').show();

            // Verifica o email submetido pelo utilizador
            if (!validateEmailAddress(jQuery('#checkout_user_input').val())){
                jQuery('#msgNotifica').html('Verifique o e-mail introduzido.');
                jQuery('#checkout_user_input').focus();
                jQuery('#msgNotifica').show();
                jQuery('#msgLoading').hide();
                return false;
            }

            // Verifica campo password
            if ( (jQuery('#checkout_password').val() == null) || (jQuery('#checkout_password').val() == '') ){
                jQuery('#msgNotifica').html('Necessário introduzir uma password.');
                jQuery('#checkout_password').focus();
                jQuery('#msgNotifica').show();
                jQuery('#msgLoading').hide();
                return false;
            }

            jQuery.post("http://www.bertrand.pt/"
                + "home/doLogin", "checkout_user_input="
                + jQuery("#checkout_user_input").val()
                + "&checkout_redirectUrl="
                + jQuery("#checkout_redirectUrl").val()
                + "&checkout_password="
                + jQuery("#checkout_password").val()
                + "&checkout_hiddenID="
                + jQuery("#checkout_hiddenID").val()+"&a=1",
                function( data ) {
                    jQuery('#msgLoading').hide();
                    jQuery('#msgNotifica').html(data);
                    jQuery('#msgNotifica').show();
                });
            return false;
        });
    });

</script>
