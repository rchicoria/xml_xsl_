            <!-- resultado da chamada ajax é colocado aqui -->
            <div id="resultContadores"></div>
            <script type="text/javascript">
                function getContadoresInfo(){
                    // get some values from elements on the page:
                    var url = "/cesto-de-compras/getContadores";

                    // Send the data using post and put the results in a div
                    jQuery.post( url,
                    function( data ) {
                        jQuery( "#resultContadores" ).html( data );
                    });
                }
                getContadoresInfo();
            </script>
